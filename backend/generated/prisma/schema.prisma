generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

model IntakeForm {
  id               Int      @id @default(autoincrement())
  mainSymptom      String
  painLevel        String
  duration         String
  otherSymptoms    String
  medications      String
  medicalHistory   String
  visualIndicators String
  createdAt        DateTime @default(now())
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
